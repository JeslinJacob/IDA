<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jeslin.ida.Home">

    <!-- TODO: Update blank fragment layout -->
   <LinearLayout
       android:orientation="vertical"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:layout_margin="20dp">


       <ImageView
           android:id="@+id/imageView5"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:layout_marginTop="20dp"
           android:layout_marginLeft="20dp"
           android:layout_marginRight="20dp"
           app:srcCompat="@mipmap/logo_ida" />


<android.support.v7.widget.CardView
    android:id="@+id/cardexec"
android:onClick="exec_members"
    android:layout_width="match_parent"
    android:layout_marginTop="30dp"

    app:cardBackgroundColor="@color/colorAccent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_margin="10dp"
        android:layout_height="wrap_content"

        android:orientation="horizontal">

        <ImageView
            android:src="@mipmap/ic_member_white"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content" />
        <TextView
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:layout_width="match_parent"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:text="Executive Members"/>

    </LinearLayout>

</android.support.v7.widget.CardView>




       <android.support.v7.widget.CardView
           android:id="@+id/cardmemb"
           android:layout_width="match_parent"
           android:layout_marginTop="30dp"
           app:cardBackgroundColor="@color/colorAccent"
           android:layout_height="wrap_content">
           <LinearLayout
               android:layout_width="match_parent"
               android:layout_margin="10dp"
               android:layout_height="wrap_content"

               android:orientation="horizontal">

               <ImageView
                   android:src="@mipmap/ic_member_white"
                   android:layout_width="wrap_content"

                   android:layout_height="wrap_content" />
               <TextView
                   android:textColor="#ffffff"
                   android:textSize="20dp"
                   android:textAlignment="center"
                   android:layout_width="match_parent"

                   android:layout_height="wrap_content"
                   android:text="Members"/>

           </LinearLayout>

       </android.support.v7.widget.CardView>

   <LinearLayout
       android:layout_width="match_parent"
       android:layout_marginTop="30dp"
       android:weightSum="3"
       android:orientation="horizontal"
       android:layout_height="90dp">

       <LinearLayout
           android:layout_weight="1"
           android:layout_marginTop="10dp"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:orientation="vertical">

<android.support.design.widget.FloatingActionButton
    android:layout_gravity="center"
    app:fabSize="normal"

    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:src="@mipmap/ic_gallery_white"
    />

           <TextView
               android:layout_width="match_parent"
               android:text="Gallery"
               android:textAlignment="center"
               android:layout_height="wrap_content" />

       </LinearLayout>

       <LinearLayout
           android:layout_weight="1"
           android:layout_marginTop="10dp"

           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:orientation="vertical">


           <android.support.design.widget.FloatingActionButton
               android:layout_gravity="center"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               app:fabSize="normal"
               android:onClick="newsclick"
               android:src="@mipmap/ic_event_white"
               />


           <TextView
               android:layout_width="match_parent"
               android:text="News and event"
               android:textAlignment="center"
               android:layout_height="wrap_content" />

       </LinearLayout>
       <LinearLayout
           android:layout_weight="1"
           android:layout_marginTop="10dp"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:orientation="vertical">


           <android.support.design.widget.FloatingActionButton
               android:layout_gravity="center"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               app:fabSize="normal"
               android:onClick="notification_click"
               android:src="@mipmap/ic_notification_white"
               />


           <TextView
               android:layout_width="match_parent"
               android:text="Notification"
               android:textAlignment="center"
               android:layout_height="wrap_content" />

       </LinearLayout>


   </LinearLayout>

   </LinearLayout>


</FrameLayout>
